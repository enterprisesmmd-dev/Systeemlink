{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Page",
    "description": "Website pagina's met secties en SEO"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "path": {
      "type": "string",
      "required": true
    },
    "sections": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::section.section"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "seo.meta"
    },
    "published": {
      "type": "boolean",
      "default": false
    }
  }
}
