{
  "name": "systeemlink-strapi",
  "private": true,
  "version": "1.0.0",
  "description": "Systeemlink CMS Backend powered by Strapi",
  "scripts": {
    "develop": "strapi develop",
    "start": "strapi start",
    "build": "strapi build",
    "strapi": "strapi",
    "migrate": "node scripts/migrate-data.js",
    "generate-secrets": "node -e \"console.log('APP_KEYS=' + Array(4).fill(0).map(() => require('crypto').randomBytes(32).toString('base64')).join(',') + '\\nAPI_TOKEN_SALT=' + require('crypto').randomBytes(32).toString('base64') + '\\nADMIN_JWT_SECRET=' + require('crypto').randomBytes(32).toString('base64') + '\\nTRANSFER_TOKEN_SALT=' + require('crypto').randomBytes(32).toString('base64') + '\\nJWT_SECRET=' + require('crypto').randomBytes(32).toString('base64'))\"",
    "health": "curl http://localhost:1337/_health || echo 'Strapi is not running'",
    "deploy-plesk": "bash deploy-plesk.sh"
  },
  "dependencies": {
    "@strapi/strapi": "4.25.12",
    "@strapi/plugin-users-permissions": "4.25.12",
    "@strapi/plugin-i18n": "4.25.12",
    "pg": "8.11.3",
    "better-sqlite3": "9.4.3"
  },
  "devDependencies": {
    "@types/node": "20.14.9",
    "@strapi/typescript-utils": "4.25.12"
  },
  "author": {
    "name": "Systeemlink",
    "email": "info@systeemlink.nl",
    "url": "https://systeemlink.nl"
  },
  "strapi": {
    "uuid": "systeemlink-cms"
  },
  "engines": {
    "node": ">=18.0.0 <=20.x.x",
    "npm": ">=6.0.0"
  },
  "license": "MIT"
}
